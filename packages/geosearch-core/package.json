{
  "name": "@opencage/geosearch-core",
  "version": "0.1.12",
  "description": "Core client for OpenCage GeoSearch",
  "author": {
    "name": "OpenCage GmbH",
    "url": "https://www.opencagedata.com"
  },
  "homepage": "https://github.com/OpenCageData/geosearch#readme",
  "bugs": {
    "url": "https://github.com/OpenCageData/geosearch/issues"
  },
  "keywords": [
    "opencage",
    "geosearch",
    "geographic",
    "searches",
    "autocomplete",
    "autosuggest",
    "type-ahead",
    "predictions"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenCageData/geosearch.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "source": "src/index.js",
  "main": "dist/js/opencage-geosearch-core.cjs.js",
  "browser": "dist/js/opencage-geosearch-core.js",
  "module": "dist/js/opencage-geosearch-core.esm.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "dist/js/opencage-geosearch-core.esm.js",
      "require": "dist/js/opencage-geosearch-core.cjs.js"
    }
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "scripts": {
    "build": "vite build",
    "build:clean": "rm -rf dist",
    "dotenv": "../../scripts/dotenv.sh $@",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "watch": "vite build --watch"
  },
  "devDependencies": {
    "@algolia/autocomplete-js": "^1.19.4",
    "@algolia/autocomplete-theme-classic": "^1.19.4"
  },
  "peerDependencies": {
    "@algolia/autocomplete-js": "^1.*",
    "@algolia/autocomplete-theme-classic": "^1.*"
  }
}
